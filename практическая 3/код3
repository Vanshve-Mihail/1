'''print('Задача 3')
n = int(input())
if n <1440:
    a=n//60
    b=n%60
    print(a,b)
else:
    while n>1440:
        n=n-1440
    d=n//60
    c=n%60
    print(d,c)'''


'''print('Задача 4')
a = int(input())
b = int(input())
l = int(input())
N = int(input())
print(2*l + a + (2*b + 2*a)*(N-1))'''

'''print('Задача 4 функция')
a = int(input())
b = int(input())
l = int(input())
N = int(input())
def f(a,b,l,N):   
    return (2*l + a + (2*b + 2*a)*(N-1))
print(f(a,b,l,N))'''

'''print('Задача 5')
a = int(input())
b = int(input())
c = int(input())
def f(a,b,c):
    return min(a,b,c)
print(f(a,b,c))'''

'''print('Задача 6')
x1 = int(input())
y1 = int(input())
x2 = int(input())
y2 = int(input())
a1 = ''
a2 = ''
if x1<8 and x2<8 and y1<8 and y2<8:
    if (x1-1)%2==0 and y1%2==0:
        a1 = 'черная'
    else:
        a1 = 'белая'
    if (x2-1)%2==0 and y2%2==0:
        a2 = 'черная'
    else:
        a2 = 'белая'
    if a1 == a2:
        print('да')
    else:
        print('нет')
else:
    print('Ошибка')'''

'''print('Задача 7')
